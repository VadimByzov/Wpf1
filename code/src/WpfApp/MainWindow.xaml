<Window x:Class="WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:local="clr-namespace:WpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="Cities">
                <Frame Source="View/CityView.xaml" />
            </TabItem>
            <TabItem Header="Streets">
                <Frame Source="View/StreetView.xaml" />
            </TabItem>
            <TabItem Header="Houses">
                <Frame Source="View/HouseView.xaml" />
            </TabItem>
            <TabItem Header="Apartments">
                <Frame x:Name="MainFrame" Source="View/ApartmentView.xaml" />
            </TabItem>
        </TabControl>
        
    </Grid>
</Window>
